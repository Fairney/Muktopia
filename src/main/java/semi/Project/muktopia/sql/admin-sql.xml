<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>admin-sql.xml</comment>
	<entry key="adminlogin">
	    SELECT MEMBER_NO,MEMBER_EMAIL
	    FROM MEMBER
	    WHERE MEMBER_EMAIL = ?
	    AND MEMBER_PW = ?
	</entry>
	<entry key="showList">
		SELECT * FROM BOARD
	</entry>
	<entry key="boardDelete">
		DELETE FROM BOARD
		WHERE "BOARD_NO" = ?
	</entry>
</properties>
    

    